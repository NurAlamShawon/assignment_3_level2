Query 1: 
select user_name , vehicle_name 
from bookings inner join users using(user_id)
inner join vehicles using(vehicle_id);


Query 2: 

select *
from vehicles 
where not exists  (
  select 1
  from bookings
  where bookings.vehicle_id=vehicles.vehicle_id
);


Query 3:

select vehicle_id from vehicles where availability_status='available';


Query 4: 

select vehicle_id , count(*) 
from bookings
group by vehicle_id 
having count(*)>2 ;



